            <link name="NotoSansThai" type="font" subtype="opentype" src="${nsfont.NotoSansThai_Regular}" src-bold="${nsfont.NotoSansThai_Bold}" bytes="2" />
        </#if>
        <macrolist>
            <macro id="nlheader">            
                <table class="header" width="100%" padding-bottom="5px" >
                    <tr>
                        <td width="25%" padding-top= "10px" >
                            <img src="https://td2995066.app.netsuite.com/core/media/media.nl?id=57182&amp;c=TD2995066&amp;h=f2ayqY9pZkGvHw5pSHr1MMpPYnHFaywnHhAA2mXbCF2IDyqh" style="width: 25%; height: 25%;"/>
                        </td>
                        <td width="25%" padding-top= "10px" > 
                            <p style="font-size:12px; font-weight:bold" align="left" valign="middle">Laporan Hasil Produksi</p>
                        </td>
                        <td width="25%" padding-top= "10px" >
                            <p style="font-size:12px;" align="left" valign="middle">Bagian: ${record.header.class_name}</p>
                        </td>
                        <td width="25%" padding-top= "10px" >
                            <p style="font-size:12px;" align="left" valign="middle">Hari/Tanggal: ${record.header.day_date}</p>
                        </td>
                    </tr>

                </table>
            </macro>
            <macro id="nlfooter">
                <table class="footer" style="width: 100%;padding-top:-30px">
                  <tr>
                        <td align="left">
                            <pagenumber/> of <totalpages/>
                        </td>
                  </tr>
                </table>
            </macro>
        </macrolist>
        <style type="text/css">* {
            <#if .locale == "zh_CN">
                font-family: Arial, sans-serif;
            <#elseif .locale == "zh_TW">
                font-family: Arial, sans-serif;
            <#elseif .locale == "ja_JP">
                font-family: Arial, sans-serif;
            <#elseif .locale == "ko_KR">
                font-family: Arial, sans-serif;
            <#elseif .locale == "th_TH">
                font-family: Arial, sans-serif;
            <#else>
                font-family: Arial, sans-serif;
            </#if>
            }
            * {
                font-size: 7px;
                  color:black;
            }
            table {
                font-size: 7px;
                table-layout: fixed;
              color:black;
            }
            .th {
                font-weight: bold;
                font-size: 8pt;
                vertical-align: middle;
                color: #333333;
            }
            td p { align:left;color:black; }
            
            table.header td {
                padding: 0px;
                font-size: 7px;
              color:black;
            }
            table.header_border {
                padding: 0px;
                vertical-align: top;
              color:black;
            }
            table.header tr:last-child{
                page-break-after: auto;
              color:black;
            }

            table.content .th {
                border-top: 1px solid;
                border-bottom: 1px solid;
                border-left: 1px solid;
                padding: 1px;
              color:black;
            }
            <!-- table.content th:first-child {
                border-left: none;
  color:black;;
            } -->
            table.content td {
                border-left: 1px solid;
                page-break-inside: auto;
                box-decoration-break: clone;
              color:black;
            }
            <!-- table.content tr {
                page-break-after: auto;
              color:black;;
            } -->
            <!-- table.content td:first-child {
                border-left: none;
              color:black;
            } -->

            table.tandatangan tr:first-child{
                page-break-after: avoid;
                page-break-before: auto;
              color:black;
            }
            table.tandatangan tr{
                page-break-after: avoid;
                page-break-before: avoid;
              color:black;
            }
            table.tandatangan tr:last-child{
                page-break-after: auto;
                page-break-before: avoid;
              color:black;
            }
            table.header_kop {
                border-collapse: collapse;
              color:black;
            }
            .white-char {
                color: white;
            }
                
        </style>
