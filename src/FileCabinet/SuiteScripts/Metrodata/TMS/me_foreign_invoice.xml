						<#elseif .locale == "ko_KR">
							font-family: NotoSans, NotoSansCJKkr, sans-serif;
						<#elseif .locale == "th_TH">
							font-family: NotoSans, NotoSansThai, sans-serif;
						<#else>
							font-family: NotoSans, sans-serif;
						</#if>
					}
					td p { align:left }
					td img { align:right }

      .content{
        margin-top: 1px;
		width: 100%;
      }
      .content-footer{
        margin-top: 1px;
		width: 100%;
      }
